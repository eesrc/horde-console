    "standard-version": "^8.0.0",
    "style-loader": "0.18.2",
    "stylelint": "^13.3.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
