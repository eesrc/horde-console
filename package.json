{
  "name": "horde-console",
  "version": "0.9.10",
  "description": "Front end for self service portal for Exploratory Engineering NB-IoT",
  "main": "dist",
  "productName": "NB-IoT Self Service Portal",
  "bundlesize": [
    {
      "path": "./dist/app.*.js",
      "maxSize": "175 kB"
    },
    {
      "path": "./dist/*chunk*.js",
      "maxSize": "75 kB"
    }
  ],
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js|json|less|css|ts|ts|md}": [
      "jest --findRelatedTests --passWithNoTests",
      "prettier --fix",
      "git add"
    ]
  },
  "scripts": {
    "server": "npm run server:dev:hmr --",
    "start": "nps",
    "test": "nps t"
  },
  "dependencies": {
    "@telenorfrontend/tn-components": "^0.1.2",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-dialog": "2.0.0-rc.3",
    "aurelia-framework": "^1.3.0",
    "aurelia-google-maps": "2.2.1",
    "aurelia-http-client": "1.3.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-templating": "^1.8.2",
    "bluebird": "3.5.1",
    "blueimp-md5": "2.10.0",
    "chart.js": "^2.7.3",
    "date-fns": "2.0.0-alpha.25",
    "js-cookie": "^2.2.0",
    "normalize.css": "^8.0.0",
    "nprogress": "0.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@exploratoryengineering/data-mapper-chain": "^0.7.1",
    "@telenorfrontend/eslint-config-telenor": "^0.1.3",
    "@telenorfrontend/stylelint-config-telenor": "^0.1.3",
    "@telenorfrontend/tn-conventions": "^0.6.3",
    "@telenorfrontend/tslint-config-telenor": "^0.1.0",
    "@types/chart.js": "^2.7.27",
    "@types/jest": "24.0.6",
    "@types/js-cookie": "^2.2.0",
    "@types/node": "8.0.17",
    "@types/nprogress": "0.0.29",
    "@types/webpack": "^4.1.4",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-loader-webpack": "2.2.1",
    "aurelia-pal-nodejs": "1.1.1",
    "aurelia-testing": "1.0.0-beta.4.0.0",
    "aurelia-tools": "^2.0.0",
    "aurelia-webpack-plugin": "3.0.0",
    "autoprefixer": "9.1.3",
    "babel-eslint": "^7.2.3",
    "bundlesize": "^0.18.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "5.0.1",
    "css-loader": "3.5.1",
    "del-cli": "^1.1.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^4.0.0",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "2.8.0",
    "jest": "^26.0.1",
